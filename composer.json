{
    "name": "spatie/laravel-directory-cleanup",
    "description": "This package will remove the expired files from the given directories. ",
    "keywords": [
        "spatie",
        "laravel-directory-cleanup"
    ],
    "homepage": "https://github.com/spatie/laravel-directory-cleanup",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Jolita Grazyte",
            "email": "jolita@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "nesbot/carbon": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench":"~3.3.0|~3.4.0|~3.5.0|~3.6.0|~3.7.0|~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\DirectoryCleanup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\DirectoryCleanup\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\DirectoryCleanup\\DirectoryCleanupServiceProvider"
            ]
        }
    }
}
